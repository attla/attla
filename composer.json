{
	"name": "attla/attla",
	"type": "project",
	"description": "The Attla Framework. An elegant PHP framework for web developers.",
	"keywords": [
		"framework",
		"attla",
		"laravel",
		"laravel-compatibility",
		"php"
	],
	"homepage": "https://octha.com/attla",
	"license": "MIT",
	"authors": [
		{
			"name": "Lucas Nicolau",
			"email": "lucas@octha.com"
		}
	],
	"require": {
		"php": ">=7.3",
		"attla/core": "dev-main",
		"attla/package-test": "dev-main",
		"attla/permission": "dev-main"
	},
	"require-dev": {
		"attla/debugbar": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"Attla\\ComposerScripts::postAutoloadDump",
			"@php attla package:discover --ansi"
		],
		"post-create-project-cmd": [
			"@php attla key:generate --ansi"
		]
	},
	"extra": {
		"attla": {
			"dont-discover": []
		}
	},
	"config": {
		"optimize-autoloader": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
